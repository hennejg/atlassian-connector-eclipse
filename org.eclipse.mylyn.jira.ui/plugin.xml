<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <extension
         id="org.eclipse.mylar.jira.repository"
         name="JIRA Repository"
         point="org.eclipse.mylar.tasks.ui.repositories">

      <repositoryType
            brandingIcon="icons/eview16/jira-logo.gif"
            class="org.eclipse.mylar.internal.jira.JiraRepositoryConnector"
            id="org.eclipse.mylar.jira.tasklist.repositories"
            name="JIRA Repository Client"
            type="jira"/>
            
            
      <externalizer 
            class="org.eclipse.mylar.internal.jira.JiraTaskExternalizer"
            id="org.eclipse.mylar.jira.tasklist.externalizer"/>
   </extension>  
   
   <extension point="org.eclipse.ui.decorators">
      <decorator
            class="org.eclipse.mylar.internal.jira.ui.JiraTaskDecorator"
            id="org.eclipse.mylar.jira.ui.decorator.task"
            label="Mylar JIRA Task Decorator"
            lightweight="true"
            objectClass="org.eclipse.mylar.tasks.core.ITaskListElement"
            state="true">
         <description>
			Mylar JIRA Task Decorator
		 </description>
      </decorator>
   </extension>
   <!--
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.eclipse.mylar.internal.jira.ui.JiraTaskEditor"
            default="false"
            id="org.eclipse.mylar.jira.task.editor"
            name="Jira Task Editor"/>
   </extension>
   -->
   <extension
         point="org.eclipse.mylar.tasks.ui.editors">
      <editorFactory
            class="org.eclipse.mylar.internal.jira.ui.JiraTaskEditorFactory"
            id="org.eclipse.mylar.jira.ui.JiraTaskEditorFactory"/>
   </extension>

</plugin>
