<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="com.atlassian.connector.eclipse.test"
      label="%featureName"
      version="0.0.0.qualifier"
      provider-name="%providerName"
      plugin="com.atlassian.connector.eclipse.ui">

   <description url="http://atlassian.com">
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="license.html">
      %license
   </license>

   <url>
      <update label="%updateSiteName" url="http://update.atlassian.com/atlassian-eclipse-plugin"/>
   </url>

   <requires>
      <import plugin="org.junit"/>
      <import plugin="com.atlassian.connector.eclipse.crucible.core"/>
      <import plugin="com.atlassian.connector.commons"/>
      <import plugin="com.atlassian.connector.eclipse.crucible.ui"/>
      <import plugin="com.atlassian.connector.eclipse.ui"/>
      <import plugin="com.atlassian.connector.eclipse.core"/>
      <import plugin="com.atlassian.connector.eclipse.bamboo.core"/>
      <import plugin="com.atlassian.connector.eclipse.bamboo.ui"/>
      <import plugin="org.eclipse.mylyn.jira.core"/>
      <import plugin="org.eclipse.mylyn.jira.ui"/>
      <import plugin="com.atlassian.connector.eclipse.fisheye.core" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="com.atlassian.connector.eclipse.fisheye.ui" version="1.0.0" match="greaterOrEqual"/>
      
      <import feature="org.eclipse.mylyn_feature" version="3.2.0.I20090529" match="compatible"/>
      <import feature="org.eclipse.mylyn.jira_feature" version="3.2.0.I20090529" match="compatible"/>
      
      <import plugin="org.eclipse.mylyn.commons.soap" version="3.2.0.I20090529" match="compatible"/>
      
      <!--
      <import plugin="com.atlassian.connector.eclipse.subclipse.ui" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="com.atlassian.connector.eclipse.subversive.ui" version="1.0.0" match="greaterOrEqual"/>
      -->
   </requires>

   <plugin
         id="com.atlassian.connector.eclipse.crucible.tests"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

	<!-- unpack=false because we refere to file from this plugin in build system -->
   <plugin
         id="com.atlassian.connector.eclipse.tests"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="true"/>

   <plugin
         id="org.jetbrains.annotations"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.atlassian.connector.commons.tests.dependencies"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.atlassian.connector.eclipse.bamboo.tests"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.atlassian.connector.eclipse.fisheye.tests"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

	<plugin id="org.eclipse.mylyn.jira.core"
		download-size="0"
		install-size="0"
		version="0.0.0"
		unpack="false"/>
		
	<plugin id="org.eclipse.mylyn.jira.ui"
		download-size="0"
		install-size="0"
		version="0.0.0"
		unpack="false"/>
		
	<plugin id="org.eclipse.mylyn.jira.tests"
		download-size="0"
		install-size="0"
		version="0.0.0"
		unpack="false"/>
		
	<plugin id="org.eclipse.mylyn.commons.soap"
		download-size="0"
		install-size="0"
		version="0.0.0"
		unpack="false"/>
		
</feature>
