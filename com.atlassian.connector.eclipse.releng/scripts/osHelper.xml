<?xml version="1.0" encoding="UTF-8"?>
<project name="os-helper" default="all" basedir=".">
	<condition property="build.system" value="linux-gtk"><and><os family="unix"/><os arch="i386"/></and></condition>
	<condition property="build.system" value="linux-gtk-ppc"><and><os family="unix"/><os arch="ppc"/></and></condition>
	<condition property="build.system" value="linux-gtk-x86_x64"><and><os family="unix"/><os arch="i686"/></and></condition>
	<condition property="build.system" value="linux-macosx-carbon"><and><os family="mac"/></and></condition>
	<condition property="build.system" value="linux-win32"><and><os family="windows"/></and></condition>
	<property file="scripts/defaults-${build.system}.properties"/>
</project>